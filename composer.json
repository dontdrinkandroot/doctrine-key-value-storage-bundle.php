{
    "name": "dontdrinkandroot/doctrine-key-value-storage-bundle",
    "type": "symfony-bundle",
    "description": "Allows to store typed key-value pairs in the Doctrine ORM.",
    "keywords": [
        "doctrine",
        "orm",
        "key-value",
        "storage"
    ],
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineKeyValueStorageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineKeyValueStorageBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/orm": "^3.6",
        "symfony/config": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/http-kernel": "^7.4"
    },
    "require-dev": {
        "brianium/paratest": "^7.16",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "liip/test-fixtures-bundle": "^3.7",
        "phpunit/phpunit": "^12.5",
        "symfony/uid": "^7.4",
        "symfony/yaml": "^7.4",
        "symfony/runtime": "^7.4"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "require": "7.4.*",
            "allow-contrib": true
        },
        "branch-alias": {
            "dev-main": "0.5.x-dev"
        }
    }
}
